[Desktop Entry]
Actions=MD5-Sum;SHA1-Sum;SHA256-Sum;SHA512-Sum;_SEPARATOR_;CheckSum;
Icon=/usr/share/icons/hicolor/scalable/apps/ks-checksum.svgz
MimeType=all/allfiles;
ServiceTypes=KonqPopupMenu/Plugin
Type=Service
X-KDE-Priority=TopLevel
X-KDE-StartupNotify=false
X-KDE-Submenu=CheckSum Tools
X-KDE-Submenu[de]=CheckSum - Werkzeuge
X-KDE-Submenu[es]=CheckSum - Herramientas
X-KDE-Submenu[fr]=CheckSum - Outils
X-KDE-Submenu[it]=CheckSum - Attrezzi
X-KDE-Submenu[ja]=検査合計 道具
X-KDE-Submenu[ko]=체크섬 도구
X-KDE-Submenu[pt]=CheckSum - Ferramentas
X-KDE-Submenu[ru]=Инструменты Контрольных Сумм
X-KDE-Submenu[uk]=Інструменти Контрольних Сум
X-KDE-Submenu[zh_CN]=核对和 工具
X-KDE-Submenu[x-test]=xxCheckSum Toolsxx

[Desktop Action CheckSum]
Exec=which md5sum sha1sum sha256sum sha512sum; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Verify CheckSum" --passivepopup="[Error] Please install md5sum, sha1sum, sha256sum and sha512sum command and try again."; exit 1; else /usr/share/applications/CheckSum_Tools-verify-checksum.sh %F;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-checksum.svgz
Name=Verify CheckSum
Name[de]=Um CheckSum zu Verifizieren
Name[es]=Verificar CheckSum
Name[fr]=Vérifier CheckSum
Name[it]=Verificare CheckSum
Name[ja]=検査合計 を確認するために
Name[ko]=체크섬을 증명한다
Name[pt]=Verificar CheckSum
Name[ru]=Проверить контрольные суммы
Name[uk]=Перевірити контрольні суми
Name[zh_CN]=查证核对和
Name[x-test]=xxVerify CheckSumxx

[Desktop Action MD5-Sum]
Exec=which md5sum; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="MD5 (strong)" --passivepopup="[Error] Please install md5sum command and try again."; exit 1; else COUNT="0"; COUNTFILES=$(echo %F|wc -w); COUNTFILES=$((++COUNTFILES)); DBUSREF=$(pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-checksum.svgz --title="CheckSum MD5" --progressbar "\t\t\t\t\t\t\t" $COUNTFILES); for file in %F;do COUNT=$((++COUNT)); qdbus $DBUSREF setLabelText "Computing:  ${file##*/}  [$COUNT/$((COUNTFILES-1))]"; qdbus $DBUSREF Set "" value $COUNT; cd ${file%/*}; md5sum "$file" > $(ls $file).md5; sed -i "s; ${file%/*}/;*;" $(ls $file).md5; pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-checksum.svgz --title="CheckSum MD5" --passivepopup="[Finished]    ${file##*/}";done; qdbus $DBUSREF Set "" value $COUNTFILES; sleep 1; qdbus $DBUSREF close;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-checksum.svgz
Name=MD5 (strong)
Name[de]=MD5 (stark)
Name[es]=MD5 (fuerte)
Name[fr]=MD5 (fort)
Name[it]=MD5 (forte)
Name[ja]=MD5 (強いです)
Name[ko]=MD5 (강한)
Name[pt]=MD5 (forte)
Name[ru]=MD5 (cтойкий)
Name[uk]=MD5 (cтійкий)
Name[zh_CN]=MD5 (强壮的)
Name[x-test]=xxMD5 (strong)xx

