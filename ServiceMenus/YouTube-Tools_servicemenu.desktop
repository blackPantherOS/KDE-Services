[Desktop Entry]
Actions=Video-Downloader;Code-Collector;
Icon=/usr/share/icons/hicolor/scalable/apps/ks-youtube-download-video.svgz
Type=Service
X-KDE-Priority=TopLevel
X-KDE-ServiceTypes=KonqPopupMenu/Plugin,inode/directory
X-KDE-StartupNotify=false
X-KDE-Submenu=YouTube Tools
X-KDE-Submenu[de]=YouTube - Werkzeuge
X-KDE-Submenu[es]=YouTube - Herramientas
X-KDE-Submenu[fr]=YouTube - Outils
X-KDE-Submenu[it]=YouTube - Attrezzi
X-KDE-Submenu[ja]=ユーチューブ道具
X-KDE-Submenu[ko]=YouTube 도구
X-KDE-Submenu[pt]=YouTube - Ferramentas
X-KDE-Submenu[ru]=Инструменты YouTube
X-KDE-Submenu[uk]=Інструменти YouTube
X-KDE-Submenu[zh_CN]=YouTube 工具
X-KDE-Submenu[x-test]=xxYouTube Toolsxx

[Desktop Action Code-Collector]
Exec=which lynx; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Video List Code Collector" --passivepopup="[Error] Please install lynx command and try again."; exit 1; else /usr/share/applications/YouTube_Tools-video-code-collector.sh %F;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-youtube-video-code-collector.svgz
Name=Video List Code Collector
Name[de]=Sammler der Codes der Liste der Videos
Name[es]=Colector de Códigos de Lista de Videos
Name[fr]=Collecteur de Codes de Liste de Vidéos
Name[it]=Collettore di Codici di Lista di Videi
Name[ja]=ビデオのリストのコードの徴収人
Name[ko]=비디오의 목록의 코드의 수집가
Name[pt]=Coletor de Códigos de Lista de Vídeos
Name[ru]=Сборщик видео по списку кодов
Name[uk]=Збиральник відео за списком кодів
Name[zh_CN]=录象机的目录密码的搜集家
Name[x-test]=xxVideo List Code Collectorxx

[Desktop Action Video-Downloader]
Exec=which youtube-dl; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Video Downloader" --passivepopup="[Error] Please install youtube-dl command and try again."; exit 1; else /usr/share/applications/YouTube_Tools-download-video.sh %F;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-youtube-download-video.svgz
Name=Video Downloader
Name[de]=Videoentlader
Name[es]=Descargador de Video
Name[fr]=Déchargeur de Vidéo
Name[it]=Scaricatore di Video
Name[ja]=ビデオ Downloader
Name[ko]=비디오 다운 로더
Name[pt]=Descarregador Vídeo
Name[ru]=Загрузчик видео
Name[uk]=Завантажувальник відео
Name[zh_CN]=录象机下载
Name[x-test]=xxVideo Downloaderxx
