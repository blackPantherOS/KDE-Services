[Desktop Entry]
Actions=SaMBa-Tools;
Icon=/usr/share/icons/hicolor/scalable/apps/ks-smbfs.svgz
Type=Service
X-KDE-Priority=TopLevel
X-KDE-ServiceTypes=KonqPopupMenu/Plugin,inode/directory
X-KDE-StartupNotify=false
X-KDE-Submenu=SaMBa Tools
X-KDE-Submenu[de]=SaMBa Werkzeuge
X-KDE-Submenu[es]=SaMBa - Herramientas
X-KDE-Submenu[fr]=SaMBa - Outils
