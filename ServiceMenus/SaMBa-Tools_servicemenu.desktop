[Desktop Action SaMBa-Tools]
Exec=which smbclient; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="SaMBa Shares Mounter" --passivepopup="[Error] Please install smbclient command and try again."; exit 1; else /usr/share/applications/SaMBa_Tools-mount-umount-share.sh %F;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-smbfs.svgz
Name=SaMBa Shares Mounter
Name[de]=Verbinder SaMBa Geteilt zu Haben
Name[es]=Montador de Compartidos SaMBa
