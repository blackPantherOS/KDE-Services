[Desktop Entry]
Actions=MultiplexSubtitle;
Icon=/usr/share/icons/hicolor/scalable/apps/ks-multiplexing-subs.svgz
MimeType=video/mpeg;
ServiceTypes=KonqPopupMenu/Plugin
Type=Service
X-KDE-StartupNotify=false
X-KDE-Submenu=Extra-Services
X-KDE-Submenu[x-test]=xxExtra-Servicesxx

[Desktop Action MultiplexSubtitle]
Exec=which ffprobe recode spumux; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Multiplex Subtitle" --passivepopup="[Error] Please install ffprobe, recode and spumux command and try again."; exit 1; else /usr/share/applications/Multiplex_Subtitle.sh %F;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-multiplexing-subs.svgz
Name=Multiplex Subtitle
