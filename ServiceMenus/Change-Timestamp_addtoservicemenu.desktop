[Desktop Entry]
Actions=ChangeTimestamp;
Icon=/usr/share/icons/hicolor/scalable/apps/ks-clock.svgz
MimeType=all/allfiles;inode/directory;
ServiceTypes=KonqPopupMenu/Plugin
Type=Service
X-KDE-StartupNotify=false
X-KDE-Submenu=Extra-Services
X-KDE-Submenu[x-test]=xxExtra-Servicesxx

[Desktop Action ChangeTimestamp]
Exec=TIMESTAMP=$(pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-clock.svgz --title="Change Timestamp to [File|Directory]" --inputbox="Enter New Timestamp" "$(date "+%0Y-%0m-%0d %0H:%0M:%0S")"); if [ "$?" != "0" ];then exit 1; else COUNT="0"; COUNTFILES=$(echo %F|wc -w); COUNTFILES=$((++COUNTFILES));  DBUSREF=$(kdialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-clock.svgz --title="Change Timestamp to [File|Directory]" --progressbar "\t\t\t\t\t\t\t" $COUNTFILES); for file in %F;do COUNT=$((++COUNT)); qdbus $DBUSREF setLabelText "Changing timestamp to ${file##*/}  [$COUNT/$((COUNTFILES-1))]"; qdbus $DBUSREF Set "" value $COUNT; cd ${file%/*}; touch -cd "$TIMESTAMP" "$file";done; qdbus $DBUSREF Set "" value $COUNTFILES; sleep 1; qdbus $DBUSREF close; kdialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-clock.svgz --title="Change Timestamp to [File|Directory]" --passivepopup="[Finished]";fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-clock.svgz
Name=Change Timestamp to [File|Directory]
Name[de]=Um Datum Davon zu Verändern das [File|Verzeichnis]
Name[es]=Cambiar Fecha del [Fichero|Directorio]
Name[fr]=Changer Date du [Fichier|Directif]
Name[it]=Cambiare Data il [Schedario|Direttivo]
Name[ja]=日付を変えることの [ファイル|ディレクトリ]
Name[ko]=날짜를 변한다 [파일|디렉토리]
Name[pt]=Mudar Data do [Arquivo|Diretório]
Name[ru]=Изменить отметку времени [Файла|Папки]
Name[uk]=Змінити відмітку часу [Файлу|Теки]
Name[zh_CN]=改变日期的 [文件|目录]
Name[x-test]=xxChange Timestamp to [File|Directory]xx
