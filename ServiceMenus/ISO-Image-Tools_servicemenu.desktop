X-KDE-Submenu[uk]=Інструменти iso9660
X-KDE-Submenu[zh_CN]=图像 iso9660 工具
X-KDE-Submenu[x-test]=xxiso9660 Image Toolsxx

[Desktop Action Burn-Image]
Exec=which wodim; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Burn iso9660 Image" --passivepopup="[Error] Please install wodim command and try again."; exit 1; else /usr/share/applications/iso_manager-burn-image.sh %F;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-media-optical-burn.svgz
Name=Burn iso9660 Image
Name[de]=Verbrennen Sie iso9660-Bild
Name[es]=Quemar Imagen iso9660
ame[fr]=Brûler Image iso9660
Name[it]=Bruciare Immagine iso9660
Name[ja]=イメージ iso9660 を燃やすために
Name[ko]=이미지 iso9660를 태운다
Name[pt]=Queimar Imagem o iso9660
Name[ru]=Записать iso9660-образ
Name[uk]=Записати iso9660-образ
Name[zh_CN]=燃烧图像 iso9660
Name[x-test]=xxBurn iso9660 Imagexx

[Desktop Action Insert-MD5sum]
Exec=which implantisomd5; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Insert MD5sum" --passivepopup="[Error] Please install implantisomd5 command and try again."; exit 1; else COUNT="0"; COUNTFILES=$(echo %F|wc -w); COUNTFILES=$((++COUNTFILES)); DBUSREF=$(kdialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-media-optical-md5sum.svgz --title="Insert MD5sum" --progressbar "\t\t\t\t\t\t\t" $COUNTFILES); for file in %F;do COUNT=$((++COUNT)); qdbus $DBUSREF setLabelText "Inserting:  ${file##*/}  [$COUNT/$((COUNTFILES-1))]"; qdbus $DBUSREF Set "" value $COUNT; cd ${file%/*}; implantisomd5 "$file" > /tmp/insert-integrity 2>&1; if [ "$?" != "0" ];then kdialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Insert MD5sum" --passivepopup="[Canceled]   ${file##*/}   $(cat /tmp/insert-integrity)";else kdialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-media-optical-md5sum.svgz --title="Insert MD5sum" --passivepopup="[Finished]   ${file##*/}   $(cat /tmp/insert-integrity)";fi;done; qdbus $DBUSREF Set "" value $COUNTFILES; sleep 1; qdbus $DBUSREF close; echo "Finish Insert Check Sum" > /tmp/speak; text2wave -F 48000 -o /tmp/speak.wav /tmp/speak; play /tmp/speak.wav; rm -fr /tmp/speak* /tmp/insert-integrity;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-media-optical-md5sum.svgz
Name=Insert MD5sum
Name[de]=Legen Sie MD5sum Ein
Name[es]=Insertar MD5sum
ame[fr]=Insérer MD5sum
Name[it]=Inserire MD5sum
Name[ja]=MD5sum を差し込むために
Name[ko]=MD5sum를 삽입한다
Name[pt]=Inserir MD5sum
Name[ru]=Вставить контрольную сумму MD5
Name[uk]=Вставити контрольну суму MD5
Name[zh_CN]=插入 MD5sum
Name[x-test]=xxInsert MD5sumxx

[Desktop Action Integrity-Check]
Exec=which checkisomd5; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Integrity Check" --passivepopup="[Error] Please install checkisomd5 command and try again."; exit 1; else COUNT="0"; COUNTFILES=$(echo %F|wc -w); COUNTFILES=$((++COUNTFILES)); DBUSREF=$(kdialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-media-optical-integrity-check.svgz --title="Integrity Check" --progressbar "\t\t\t\t\t\t\t" $COUNTFILES); for file in %F;do COUNT=$((++COUNT)); qdbus $DBUSREF setLabelText "Checking:  ${file##*/}  [$COUNT/$((COUNTFILES-1))]"; qdbus $DBUSREF Set "" value $COUNT; cd ${file%/*}; checkisomd5 "$file" > /tmp/check-integrity 2>&1; if [ "$?" != "0" ];then kdialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Integrity Check" --passivepopup="[Canceled]   ${file##*/}   $(cat /tmp/check-integrity|grep -v Press)";else kdialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-media-optical-integrity-check.svgz --title="Integrity Check" --passivepopup="[Finished]   ${file##*/}   $(cat /tmp/check-integrity|grep -v Press)";fi;done; qdbus $DBUSREF Set "" value $COUNTFILES; sleep 1; qdbus $DBUSREF close; echo "Finish Integrity Check" > /tmp/speak; text2wave -F 48000 -o /tmp/speak.wav /tmp/speak; play /tmp/speak.wav; rm -fr /tmp/speak* /tmp/check-integrity;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-media-optical-integrity-check.svgz
Name=Integrity Check
Name[de]=Integritätsscheck
Name[es]=Comprobar Integridad
ame[fr]=Vérifier Intégrité
Name[it]=Comprovare Integrità
Name[ja]=完全性をチェックするために
Name[ko]=성실을 점검한다
Name[pt]=Conferir Integridade
Name[ru]=Проверить целостность
Name[uk]=Перевірити цілісність
Name[zh_CN]=检查正直
Name[x-test]=xxIntegrity Checkxx

