[Desktop Action ISOBuild]
Exec=which genisoimage; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Build iso9660 Image from Here" --passivepopup="[Error] Please install genisoimage command and try again."; exit 1; else DBUSREF=$(pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-media-optical-mount.svgz --title="Build iso9660 Image from Here" --progressbar "\t\t\t\t\t\t\t" /ProgressDialog); qdbus $DBUSREF setLabelText "Building:  $(basename %f.iso)"; genisoimage -R -J -o %f.iso %f; qdbus $DBUSREF close; echo "Finish Build Image" > /tmp/speak; text2wave -F 48000 -o /tmp/speak.wav /tmp/speak; play /tmp/speak.wav; rm -fr /tmp/speak*; pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-media-optical-mount.svgz --title="Build iso9660 Image from Here" --passivepopup="[Finished]   $(basename %f.iso)";fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-media-optical-mount.svgz
Name=Build iso9660 Image from Here
Name[de]=Bauen Sie von Hier iso9660-Bild
Name[es]=Construir Imagen iso9660 Desde Aquí
