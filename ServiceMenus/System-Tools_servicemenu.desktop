[Desktop Action XorgConf]
Exec=which lspci gtf; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Xorg Configure" --passivepopup="[Error] Please install lspci and gtf command and try again."; exit 1; else kdesu -i /usr/share/icons/hicolor/scalable/apps/ks-xorg.svgz --noignorebutton -d /usr/share/applications/System_Tools-Xorg-configure.sh %F;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-xorg.svgz
Name=Xorg Configure
