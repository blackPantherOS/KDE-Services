[Desktop Entry]
Actions=BuildKernel;CheckKernelUpdate;RebuildPackage;SysInfo;SysMonitor;Htop;XorgConf;
Icon=/usr/share/icons/hicolor/scalable/apps/ks-hwinfo.svgz
Type=Service
X-KDE-Priority=TopLevel
X-KDE-ServiceTypes=KonqPopupMenu/Plugin,inode/directory
X-KDE-StartupNotify=false
X-KDE-Submenu=System Tools
X-KDE-Submenu[de]=System - Werkzeuge
X-KDE-Submenu[es]=Sistema - Herramientas
X-KDE-Submenu[fr]=Système - Outils
X-KDE-Submenu[it]=Sistema - Attrezzi
X-KDE-Submenu[ja]=システム道具
X-KDE-Submenu[ko]=시스템 도구
X-KDE-Submenu[pt]=Sistema - Ferramentas
X-KDE-Submenu[ru]=Инструменты системы
X-KDE-Submenu[uk]=Інструменти системи
X-KDE-Submenu[zh_CN]=系统工具
X-KDE-Submenu[x-test]=xxSystem Toolsxx

[Desktop Action BuildKernel]
Exec=which gcc make ccache rpmbuild rpm yum yumdownloader yum-builddep rpmdev-setuptree; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Build Custom Kernel" --passivepopup="[Error] Please install ccache, gcc, make, rpm. rpmbuild, rpmdev-setuptree, yum-builddep, yum and yumdownloader command and try again."; exit 1; else xterm -hold -si -s -sl 1000000 -sb -T "Build Custom Kernel - Start time $(date +%H"h":%M"m")" -bg black -fg white -e /usr/share/applications/System_Tools-build-custom-kernel.sh %F;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-kernel-rebuild.svgz
Name=Build Custom Kernel
Name[de]=Um Eine Persönliche Note Gegebenen Kern zu Bauen
Name[es]=Construir Kernel Personalizado
Name[fr]=Construire Kernel Personnalisé
Name[it]=Costruire Kernel Personalizzato
Name[ja]=個人化されたカーネルを作るために
Name[ko]=개인전용의 커늘을 형성한다
Name[pt]=Construir Núcleo Personalizado
Name[ru]=Собрать собственное ядро
Name[uk]=Зібрати власне ядро
Name[zh_CN]=建立个人化的核心
Name[x-test]=xxBuild Custom Kernelxx

[Desktop Action CheckKernelUpdate]
Exec=which yumdownloader; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Check Kernel Update" --passivepopup="[Error] Please install yumdownloader command and try again."; exit 1; else /usr/share/applications/System_Tools-check-kernel-update.sh %F;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-kernel-update.svgz
Name=Check Kernel Update
Name[de]=Scheck Kern Aktualisierung
Name[es]=Chequear Actualización del Kernel
Name[fr]=Vérifiez Actualisation du Kernel
Name[it]=Controlli Aggiornamento di Nocciolo
Name[ja]=チェック核更新
Name[ko]=수표 커늘 갱신
Name[pt]=Confira Atualização de Núcleo
Name[ru]=Проверить обновления ядра
Name[uk]=Перевірити оновлення ядра
Name[zh_CN]=检查核心更新
Name[x-test]=xxCheck Kernel Updatexx

[Desktop Action Htop]
Exec=which htop; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Process Viewer" --passivepopup="[Error] Please install htop command and try again."; exit 1; else /usr/share/applications/System_Tools-process-viewer.sh %F;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-system-process.svgz
Name=Process Viewer
Name[de]=Finder der Prozesse
Name[es]=Visor de Procesos
Name[fr]=Viseur de Procès
Name[it]=Mirino di Processi
Name[ja]=プロセスのファインダ
Name[ko]=과정의 파인더
Name[pt]=Descobridor de Processos
Name[ru]=Обзор процессов
Name[uk]=Перегляд процесів
Name[zh_CN]=程序的探测器
Name[x-test]=xxProcess Viewerxx

[Desktop Action RebuildPackage]
Exec=which ccache gcc make pidof rpm rpmdev-setuptree setterm yum yumdownloader; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Rebuild RPM Package" --passivepopup="[Error] Please install ccache, gcc, make, pidof, rpm, rpmdev-setuptree, setterm, yum and yumdownloader command and try again."; exit 1; else xterm -hold -si -s -sl 1000000 -sb -T "Rebuild RPM Package - Start time $(date +%H"h":%M"m")" -bg black -fg white -e /usr/share/applications/System_Tools-rebuild-package.sh %F;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-rebuild-rpm.svgz
Name=Rebuild RPM Package
Name[de]=Um Paketdrehzahl zu Rekonstruieren
Name[es]=Reconstruir Paquete RPM
Name[fr]=Reconstruire Paquet RPM
Name[it]=Ricostruire Pacchetto RPM
Name[ja]=パッケージ RPM を再構築するために
Name[ko]=꾸러미 RPM 를 재건한다
Name[pt]=Reconstruir RPM de Pacote
Name[ru]=Пересобрать пакет RPM
Name[uk]=Перезібрати пакунок RPM
Name[zh_CN]=重建包裹 RPM
Name[x-test]=xxRebuild RPM Packagexx

