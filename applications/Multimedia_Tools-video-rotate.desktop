[Desktop Entry]
Categories=Multimedia-Tools;
Exec=which ffmpeg; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Rotate Video Files" --passivepopup="[Error] Please install ffmpeg command and try again."; exit 1; else /usr/share/applications/ffmpeg_multifile-video-rotate.sh;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-video-rotate.svgz
Name=Rotate Video Files
Name[de]=Um Videoakten zu Drehen
Name[es]=Rotar Ficheros de Video
Name[fr]=Rouler Fichiers de Vidéo
Name[it]=Ruotare Schedari di Video
Name[ja]=ビデオファイルを回転させるために
Name[ko]=차형의 비디오 파일에
Name[pt]=Girar Arquivos Vídeos
Name[ru]=Вращайтесь Видео Файлы
Name[uk]=Обертатися Вiдео Файли
Name[zh_CN]=替换电视的文件
Path=/usr/share/applications
StartupNotify=true
Terminal=false
Type=Application
X-KDE-SubstituteUID=false
