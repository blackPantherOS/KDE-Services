[Desktop Entry]
Categories=SSH-Tools;
Exec=which sshfs; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="Mount point to Remote Directory" --passivepopup="[Error] Please install sshfs command and try again."; exit 1; else /usr/share/applications/SSH_Tools-sshfs.sh;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-sshfs.svgz
Name=Mount point to Remote Directory
Name[de]=Um Entfernt Verzeichnis zu Montieren
Name[es]=Montar Directorio Remoto
Name[fr]=Monter Directive Lointaine
Name[it]=Montare Direttiva Remota
Name[ja]=ディレクトリリモートコントロールを取り付けるために
Name[ko]=멀리 떨어지는 디렉토리에 오른다
Name[pt]=Montar Diretório Remoto
Name[ru]=Смонтировать удалённую папку
Name[uk]=Змонтувати віддалену теку
Name[zh_CN]=装目录遥远的
NoDisplay=false
StartupNotify=true
Terminal=false
Type=Application
X-KDE-SubstituteUID=false
