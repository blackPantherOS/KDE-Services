[Desktop Entry]
Categories=System-Tools;
Exec=which dmidecode; if [ "$?" != "0" ];then pydialog --icon=/usr/share/icons/hicolor/scalable/apps/ks-error.svgz --title="System Information" --passivepopup="[Error] Please install dmidecode command and try again."; exit 1; else /usr/share/applications/System_Tools-sys-info.sh;fi
Icon=/usr/share/icons/hicolor/scalable/apps/ks-hwinfo.svgz
Name=System Information
Name[de]=Informationen des Systemes
Name[es]=Información del Sistema
Name[fr]=Information du Système
Name[it]=Informazione del Sistema
Name[ja]=システムのインフォメーション
Name[ko]=체내의 정보
Name[pt]=Informação do Sistema
Name[ru]=Информация о системе
Name[uk]=Інформація про систему
Name[zh_CN]=系统的数据
StartupNotify=true
Terminal=false
Type=Application
X-KDE-SubstituteUID=false
